    "autoprefixer": "^10.4.8",
    "flowbite-typography": "^1.0.2",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.5.0",
    "rollup": "^2.78.0",
    "subfont": "^6.9.0",
